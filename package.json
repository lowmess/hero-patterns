  "repository": {
    "type": "git",
    "url": "https://github.com/lowmess/hero-patterns.git"
  },
  "author": "Alec Lomas",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015-rollup": "^3.0.0",
    "cross-env": "^3.2.4",
    "eslint": "^3.19.0",
    "eslint-config-lowmess": "^1.1.0",
    "fs-extra": "^2.1.2",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.7.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^2.0.0"

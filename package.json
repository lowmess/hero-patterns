{
  "name": "hero-patterns",
  "version": "1.3.4",
  "description": "Patterns from heropatterns.com",
  "main": "dist/hero-patterns.cjs.js",
  "module": "dist/hero-patterns.es.js",
  "scripts": {
    "bundle": "rollup -c",
